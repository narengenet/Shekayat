                    refreshDeleted();
                } else {
                    failureDeleteThread();
                }

            }).fail(function (jqXHR, textStatus) {
                failureDeleteAdmin();
            });
        }

        function deletePost() {
            //alert($(this).val());
            var obj = {
                'adminid': adminid,
