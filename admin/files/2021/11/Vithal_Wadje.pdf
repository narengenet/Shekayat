                dataType: "json",
                data: obj,
                responseType: "json",
            }).done(function (response) {
                if (response == "1") {
                    //successDeleteThread();
                    refreshDeleted();
                } else {
                    failureDeletePost();
                }

            }).fail(function (jqXHR, textStatus) {
                failureDeleteAdmin();
            });
