        function deleteThread() {
            //alert($(this).val());
            var obj = {
                'adminid': adminid,
                'threadid': threadid_delete
            }
            $.ajax({
                url: "../handlers/deleteThread.ashx",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: obj,
                responseType: "json",
            }).done(function (response) {
                if (response == "1") {
