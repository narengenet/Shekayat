    <script>
        // Read a page's GET URL variables and return them as an associative array.
        function getUrlVars() {
            var vars = [], hash;
            var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
            for (var i = 0; i < hashes.length; i++) {
                hash = hashes[i].split('=');
                vars.push(hash[0]);
                vars[hash[0]] = hash[1];
            }
            return vars;
        }

        function fadeBackground(item,newcolor) {
            // Change background
            var original = $(item).css('background-color');
            $(item).css('background-color', newcolor);


            // Wait 1.2 seconds, then remove the row
            setTimeout(function () {
                $(item).css('background-color',original);
            }, 5000);
        }
    </script>


